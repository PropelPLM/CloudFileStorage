{
    "name": "driveuploader",
    "version": "1.0.0",
    "description": "Upload files from local storage onto Google Drive.",
    "main": "index.js",
    "engines": {
        "npm": "6.14.7",
        "node": "18.6.0"
    },
    "scripts": {
        "start": "node build/v1/main.js PRODUCTION",
        "dev": "node build/v1/main.js",
        "postinstall": "tsc && rm -v -rf src",
        "test": "jest",
        "test:watch": "jest --watch --no-cache --verbose --detectOpenHandles --runInBand --colors"
    },
    "author": "clinton",
    "license": "ISC",
    "dependencies": {
        "@aws-sdk/client-cloudfront": "^3.137.0",
        "@aws-sdk/client-s3": "3.13.1",
        "@aws-sdk/lib-storage": "3.13.1",
        "@aws-sdk/s3-request-presigner": "3.13.1",
        "@microsoft/microsoft-graph-client": "^2.2.1",
        "axios": "^0.21.1",
        "busboy": "^0.3.1",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "formidable": "^1.2.2",
        "googleapis": "^94.0.0",
        "isomorphic-fetch": "^3.0.0",
        "jquery": "^3.4.1",
        "jsforce-propel": "^1.9.3",
        "multer": "^1.4.2",
        "progress-stream": "^2.0.0",
        "qs": "^6.9.6",
        "redis": "^4.1.0",
        "socket.io": "^2.3.0",
        "socketio": "^1.0.0",
        "typescript": "^4.5.5",
        "uuid": "^8.3.2",
        "xlsx-populate": "^1.21.0"
    },
    "devDependencies": {
        "@babel/core": "^7.9.6",
        "@babel/preset-env": "^7.9.6",
        "@babel/preset-typescript": "^7.9.0",
        "@types/busboy": "^0.2.3",
        "@types/cors": "^2.8.6",
        "@types/express": "^4.17.6",
        "@types/express-serve-static-core": "^4.17.7",
        "@types/gapi": "0.0.39",
        "@types/gapi.auth2": "0.0.52",
        "@types/gapi.client.drive": "^3.0.4",
        "@types/jest": "^25.2.3",
        "@types/jsforce": "^1.9.18",
        "@types/lodash": "^4.14.150",
        "@types/node": "^14.0.5",
        "@types/redis": "^4.0.11",
        "@types/socket.io": "^2.1.8",
        "@types/supertest": "^2.0.9",
        "@types/uuid": "^8.3.4",
        "@types/xlsx-populate": "git+https://github.com/JanLoebel/types-xlsx-populate.git",
        "babel-jest": "^26.0.1",
        "jest": "^26.0.0",
        "supertest": "^4.0.2",
        "ts-jest": "^26.0.0",
        "tslint": "^6.1.2"
    },
    "jest": {
        "transform": {
            "^.+\\.[jt]sx?$": "ts-jest"
        },
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
        "moduleFileExtensions": [
            "ts",
            "js",
            "json",
            "node"
        ],
        "setupFilesAfterEnv": [
            "<rootDir>/jest.extend.ts",
            "<rootDir>/jest-setup.js"
        ],
        "transformIgnorePatterns": [
            "<rootDir>/node_modules/"
        ],
        "modulePathIgnorePatterns": [
            "<rootDir>/src/v0/__tests__/platforms/GoogleDrive/GoogleDrive.test.ts",
            "<rootDir>/src/v0/__tests__/routers/authRouter.test.ts",
            "<rootDir>/src/v0/__tests__/routers/uploadRouter.test.ts"
        ]
    }
}
